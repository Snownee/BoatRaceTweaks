{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Provides utilities and improves the maneuverability of the boats so that more people can enjoy the fun of boat racing.",
  "authors": [
    "Snownee",
    "SettingDust"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/boat-tweaks",
    "sources": "$source",
    "issues": "${source}/issues"
  },
  "license": "ARR",
  "entrypoints": {
    "main": [
      "snownee.boattweaks.util.CommonProxy"
    ],
    "client": [
      "snownee.boattweaks.util.ClientProxy"
    ]
  },
  "mixins": [
    "boattweaks.mixins.json"
  ],
  "accessWidener": "boattweaks.accesswidener",
  "depends": {
    "fabric-api": "*",
    "kiwi": "$kiwi",
    "minecraft": "$minecraft",
    "java": "$java"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/rRBvGkz"
      }
    }
  }
}
